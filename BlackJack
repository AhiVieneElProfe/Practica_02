#include <stdio.h>
#include <time.h>
#include <stdlib.h>

void printCarta(int carta);

int main(){
	
	int baraja[52], i, carta = 0;
	for(i=0; i <52; i++){
		if(i%4==0){
			carta++;
		}
		baraja [i] = carta; 
		
		printCarta(baraja[i]);
		
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	int n;
   time_t t;
   
   n = 5;
   
   /* Intializes random number generator */
   srand((unsigned) time(&t));

   /* Print 5 random numbers from 0 to 50 */
   for( i = 0 ; i < n ; i++ ) {
      printf("%d\n", rand() % 52);
   }
	
	
	
	srand(time(NULL));
	int maxx = 51;
	int minn = 0;
	int valor = minn + rand()/(RAND_MAX/(maxx-minn+1)+1);
	printf("%d", valor);
	printCarta(baraja[valor]);
	
	
			
	}
	
	void printCarta(int carta){
		switch(carta){
			case 1:
				printf("A");
				break;
			case 11:
				printf("J");
				break;
			case 12:
				printf("Q");
				break;
			case 13:
				printf("K");
				break;
			default:
				printf("%d", carta);			
		}	
	}
	
	int obtenerValorCarta(int carta){
		if(carta > 10){
			return 10;
		}
		return carta;
		
	}
