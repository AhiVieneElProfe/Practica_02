#include <stdio.h>
#include <time.h>
#include <stdlib.h>

int main(){
	
	srand(time(NULL));
	int  carta[51], total =  0;
	int max = 51, min = 0;
	int valor = min + rand()/(RAND_MAX/(max-min+1)+1);
	printf("BLACK  % d", valor);

	
		for(int cont2 = 0; cont2 < 51; cont2 ++){
			carta[cont2] = cont2;
			printf("%d\n", carta[cont2]);
		}
        //LLENAR EL VECTOR
		
printf("Tus cartas son %d y %d", carta[valor],carta[valor]); 
			
	}	
	
	
	
	
}
